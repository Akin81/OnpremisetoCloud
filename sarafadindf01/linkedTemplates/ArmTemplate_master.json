{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sarafadindf01"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"link to raw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'link to raw'"
		},
		"AzureSqlDatabade_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabade'"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-40U6G1H\\SQLEXPRESS"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Layoffs"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "haykay"
		},
		"AzureSqlDatabade_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "haykayserver.database.windows.net"
		},
		"AzureSqlDatabade_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "haykaydata"
		},
		"AzureSqlDatabade_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "haykay"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sarafadindf01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"link to raw_connectionString": {
						"value": "[parameters('link to raw_connectionString')]"
					},
					"AzureSqlDatabade_password": {
						"value": "[parameters('AzureSqlDatabade_password')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"AzureSqlDatabade_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabade_properties_typeProperties_server')]"
					},
					"AzureSqlDatabade_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabade_properties_typeProperties_database')]"
					},
					"AzureSqlDatabade_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabade_properties_typeProperties_userName')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}